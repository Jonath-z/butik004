<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="/static/butik.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Butik</title>
</head>
<body>
    <section class="specialCommand">
    <% shoes.reverse().forEach(i => { %>
        <%- `<div class="image"><img src="/customers/${i.image}" id="commandShoes" alt="image"><p class="details">${i.Name}<br>${i.PostName}<br>${i.Phone}<br>${i.Contry}<br>${i.Email}<br>${i.Comment}<br>${i.Date}</p></div>` %> 
<% }) %>
</section>
</body>
</html>