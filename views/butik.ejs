<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Butik</title>
</head>
<body>
    <section class="butikForm"></section>
    <form action="/uploads/butik" method="post" enctype="multipart/form-data">
    <div class="uploadingFile">
        <label>Image</label>
        <input type="file" name="Image" id="File">
    </div>
    <div class="uploadingSize">
        <label for="text">size</label>
        <input type="text" name="size" id="size">    
    </div>
    <div class="uploadingModel">
        <label for="model">Model</label>
        <input type="text" name="model" id="Model">
    </div>
    <div>
        <label for="price">Price</label>
        <input type="number" name="price" id="price">
    </div>
    <div>
        <button type="submit">Post on Butik</button>
    </div>
</form> 
</section>
<secton class="reportCommand">
    <p class="commandNumber">
        command until here : <%- number %> commands 
    </p>
    <% command.reverse().forEach(command => {%>
       <%- `<div class="userDetails"><pclass="user">Name: ${command.Name}</p><p class="user">PostName: ${command.PostName} </p><p class="user">Phone: ${command.Phone}</p><p class="user">Email:${command.Email} </p><p class="user">Contry: ${command.Contry}</p></div><div class="userShoes"><img src="/public/${command.Command}" alt="image" class="user"></div><hr>` %>
        <% })%> 
</secton>
</body>
</html>